{
  "name": "discojs",
  "version": "1.1.0",
  "description": "Easiest way to use the Discogs API in Javascript",
  "author": "aknorw",
  "license": "MIT",
  "keywords": [
    "discogs",
    "api",
    "client"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "pretty": "prettier --config .prettierrc --write src/**/*.js",
    "lint": "eslint --ext .js src",
    "test": "cross-env NODE_ENV=test nyc --reporter=text mocha ./test ./src/**/*.test.js ./src/**/**/*.test.js --require @babel/register --timeout 10000",
    "test:ci": "npm run test && nyc report --reporter=text-lcov | coveralls",
    "prebuild": "yarn run clean",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aknorw/discojs.git"
  },
  "homepage": "https://github.com/aknorw/discojs#readme",
  "bugs": {
    "url": "https://github.com/aknorw/discojs/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "bottleneck": "^2.19.1",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "core-js": "3",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.15.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^5.0.0"
  }
}
